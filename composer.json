{
    "name": "boreus/type-safe-collection",
    "description": "A thin wrapper around Illuminate/Collection that allows for enforcing type constraints on the collection elements",
    "license": "MIT",
    "require": {
        "illuminate/support": ">=7.0",
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "mockery/mockery": "*",
        "satooshi/php-coveralls": "*"
    },
    "keywords": [
      "laravel",
      "collection",
      "type safe",
      "illuminate",
      "array"
    ],
    "authors": [
        {
            "name": "Jonathan Crowe",
            "email": "jcrowe206@gmail.com"
        },
        {
            "name": "Boris de Wilde",
            "email": "boreus@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
      "psr-4": {
        "JCrowe\\TypeSafeCollection\\": "src/",
        "Illuminate\\Support\\": "vendor/illuminate/support"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "JCrowe\\Tests\\Repositories\\": "tests/"
      }
    },
    "prefer-stable": true
}
